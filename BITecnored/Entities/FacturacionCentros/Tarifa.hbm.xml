    assembly="BITecnored"
    namespace="BITecnored.Entities.FacturacionCentros">
  <class name="Tarifa" table="tarifas_centros">
    <id name="id" column="id" />
    <property name="nombre" column="nombre"/>
    <property name="tarifa_por_inspeccion" column="tarifa_por_inspeccion"/>
    <property name="periodo_desde" column="periodo_desde"/>
    <property name="periodo_hasta" column="periodo_hasta"/>
    <property name="color_vista" column="color_vista"/>
    <property name="observacion" column="observacion"/>
    <property name="fecha_creacion" column="fecha_creacion"/>
    <property name="usuario_creacion" column="usuario_creacion"/>
  </class>
</hibernate-mapping>